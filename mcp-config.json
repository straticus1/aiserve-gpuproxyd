{
  "mcpServers": {
    "aiserve-gpuproxy": {
      "command": "curl",
      "args": [
        "-X",
        "POST",
        "-H",
        "Content-Type: application/json",
        "-H",
        "X-API-Key: YOUR_API_KEY_HERE",
        "http://localhost:8080/api/v1/mcp"
      ],
      "description": "AIServe GPU Proxy - Manage GPU instances and track spending",
      "metadata": {
        "provider": "aiserve.farm",
        "version": "1.0.0",
        "capabilities": [
          "gpu-management",
          "billing",
          "guard-rails"
        ]
      }
    }
  }
}
